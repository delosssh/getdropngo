{
  "name": "angular2-full-stack",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build -env=prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --host 0.0.0.0 --port 4200 --disableHostCheck true \" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -prod -aot && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "postinstall": "ng build -prod -aot"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.2.0",
    "@angular/animations": "5.1.1",
